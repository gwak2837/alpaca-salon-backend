{
  "name": "alpacasalon-backend",
  "version": "0.1.0",
  "description": "alpacasalon-backend",
  "homepage": "https://github.com/rmfpdlxmtidl/alpaca-salon-backend#readme",
  "bugs": {
    "url": "https://github.com/rmfpdlxmtidl/alpaca-salon-backend/issues",
    "email": "alpacasalon@alpacasalon.com"
  },
  "license": "GPL-3.0-only",
  "author": "Alpaca Salon <alpacasalon@alpacasalon.com>",
  "main": "dist/index.js",
  "repository": "https://github.com/rmfpdlxmtidl/alpaca-salon-backend.git",
  "scripts": {
    "clean": "rm -rf .yarn/cache .yarn/unplugged .yarn/install-state.gz coverage dist .pnp.cjs yarn.lock",
    "dev": "NODE_ENV=development concurrently \"webpack\" \"nodemon -r dotenv/config dist/index.js dotenv_config_path=.env.development --trace-warnings\"",
    "build": "yarn generate && NODE_ENV=production webpack",
    "start": "NODE_ENV=production node -r dotenv/config dist/index.js",
    "test": "NODE_ENV=test jest",
    "format": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "type": "tsc --pretty --noEmit",
    "pre-push": "yarn generate && yarn format && yarn type",
    "generate": "rm -rf src/graphql/generated && graphql-codegen --config codegen.yml",
    "generate-db": "database/generate",
    "export-db": "database/export",
    "import-db": "database/import",
    "postinstall": "[ -d '.husky' ] && husky install || true"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.16.1",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-server-core": "^3.5.0",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.7.2",
    "graphql-scalars": "^1.13.6",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "node-fetch": "^3.1.0",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.7.1",
    "redis": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/pg": "^8.6.1",
    "@types/redis": "^2.8.32",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vramework/schemats": "^0.0.12",
    "axios": "^0.24.0",
    "babel-jest": "^27.4.2",
    "concurrently": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=16.13.1"
  },
  "private": true
}
